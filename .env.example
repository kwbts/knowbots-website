# Example environment variables for Knowbots website
# Copy this file to .env and fill in your actual values

# Supabase configuration
# Required for Nuxt Supabase module and direct storage access
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-anon-key-here

# For server-side API (service role has more permissions)
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Storage configuration - these match the names in your Supabase storage
STORAGE_BUCKET=may-core-sample
TARGET_FILE=core-sample-may-2025.json

# Site configuration
NUXT_PUBLIC_SITE_URL=https://knowbots.com